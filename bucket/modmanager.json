{
    "version": "1.1.0",
    "description": "A Qt-based mod manager for minecraft.",
    "homepage": "https://github.com/kaniol-lck/modmanager/",
    "license": "GPL-3.0-only",
    "architecture": {
        "64bit": {
            "url": "https://github.com/kaniol-lck/modmanager/releases/download/v1.1.0/ModManager-v1.1.0-build.535-x64-Installer.exe"
        },
        "32bit": {
            "url": "https://github.com/kaniol-lck/modmanager/releases/download/v1.1.0/ModManager-v1.1.0-build.535-Installer.exe"
        }
    },
    "installer": {
        "args": [
            "/verysilent",
            "/nocancel",
            "/norestart",
            "/dir=$dir"
        ]
    },
    "uninstaller": {
        "file": "uninst.exe",
        "args": [
            "/verysilent",
            "/nocancel",
            "/norestart"
        ]
    },
    "bin": "modmanager.exe",
    "shortcuts": [
        [
            "modmanager.exe",
            "ModManager"
        ]
    ],
    "checkver": {
        "url": "https://github.com/kaniol-lck/modmanager/releases",
        "regex": "ModManager-v(?<version>[\\d.]+)-/build.[0-9]+/-",
        "replace": "${version}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/kaniol-lck/modmanager/releases/download/v$version/$matchName-x64-Installer.exe"
            },
            "32bit": {
                "url": "https://github.com/kaniol-lck/modmanager/releases/download/v$version/$matchName-Installer.exe"
            }
        }
    }
}
