{
    "version": "1.1.0",
    "description": "A Qt-based mod manager for minecraft.",
    "homepage": "https://github.com/kaniol-lck/modmanager/",
    "license": "GPL-3.0-only",
    "architecture": {
        "64bit": {
            "url": "https://github.com/kaniol-lck/modmanager/releases/download/v1.1.0/ModManager-v1.1.0-build.535-x64-Installer.exe#/setup.exe",
            "hash": "sha1:B8AC60D3117CA5AB2EFEB45A09C7F23B9E7F89F3"
        },
        "32bit": {
            "url": "https://github.com/kaniol-lck/modmanager/releases/download/v1.1.0/ModManager-v1.1.0-build.535-Installer.exe#/setup.exe",
            "hash": "sha1:AB4AF9C130CBB03921D52F4C95359837D7040986"
        }
    },
    "installer": {
        "script": [
            "Invoke-ExternalCommand \"$dir\\setup.exe\" -ArgumentList @('/S', \"/D=$dir\") -RunAs | Out-Null"
        ]
    },
    "uninstaller": {
        "script": "Invoke-ExternalCommand \"$dir\\uninst.exe\" -ArgumentList @('/S') -RunAs | Out-Null"
    },
    "bin": "modmanager.exe",
    "shortcuts": [
        [
            "modmanager.exe",
            "ModManager"
        ]
    ],
    "checkver": {
        "url": "https://github.com/kaniol-lck/modmanager/releases",
        "regex": "ModManager-v(?<version>[\\d.]+)-/build.[0-9]+/-",
        "replace": "${version}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/kaniol-lck/modmanager/releases/download/v$version/$matchName-x64-Installer.exe#/setup.exe"
            },
            "32bit": {
                "url": "https://github.com/kaniol-lck/modmanager/releases/download/v$version/$matchName-Installer.exe#/setup.exe"
            }
        }
    }
}
